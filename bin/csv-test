#!/usr/bin/env node

var fs = require('fs'),
    path = require('path'),
    args = process.argv.slice(2),
    config = fs.readFileSync(path.resolve(process.cwd(), args[0]), 'utf8'),
    validators = args[2] && require(path.resolve(process.cwd(), args[2]));

require('..')(config, args[1], validators);
